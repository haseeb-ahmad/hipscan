%h1 Listing Professional Service Requests

%table.scaffold
  %tr.th
    %th Service
    %th Date
    %th Name
    %th Email
    %th Industry
    %th

  - @service_requests.each do |service_request|
    %tr{:class => cycle('odd', 'even')}
      %td= service_request.service_type
      %td= service_request.created_at.strftime("%c")
      %td= service_request.name
      %td= service_request.email
      %td= service_request.industry
      %td= link_to 'Destroy', admin_service_request_path(service_request), :confirm => 'Are you sure?', :method => :delete

%br

